  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext", 
    "moduleResolution": "NodeNext",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,       
    "noEmit": true
  }
}